    "@docsearch/css": "^3.6.0",
    "@docsearch/react": "^3.6.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "ansi-to-html": "^0.7.2",
    "astro": "^4.6.3",
    "dir-compare": "^4.2.0",
    "front-matter": "^4.0.2",
    "preact": "^10.20.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.75.0"
  },
