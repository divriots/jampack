    "@types/node": "^18.0.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^18.0.0",
    "ansi-to-html": "^0.7.2",
    "astro": "^2.0.6",
    "dir-compare": "^4.0.0",
    "front-matter": "^4.0.2",
    "preact": "^10.7.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.56.1",
