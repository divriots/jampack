{
  "name": "@divriots/jampack",
  "version": "0.15.0",
  "cache-version": {
    "img": "v1",
    "img-ext": "v1"
  },
  "description": "Packer for static websites",
  "author": "Georges Gomes @georges-gomes",
  "bin": {
    "jampack": "dist/index.js"
  },
  "homepage": "https://github.com/divriots/jampack",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "repository": "divriots/jampack",
  "files": [
    "/bin",
    "/dist"
  ],
  "dependencies": {
    "@divriots/cheerio": "1.0.0-rc.12",
    "@proload/core": "^0.3.3",
    "@swc/core": "^1.3.80",
    "commander": "^10.0.1",
    "deepmerge": "^4.3.1",
    "file-type": "^18.5.0",
    "globby": "^13.2.2",
    "hasha": "^5.2.2",
    "html-minifier-terser": "^7.2.0",
    "kleur": "^4.1.5",
    "lightningcss": "^1.21.5",
    "mini-svg-data-uri": "^1.4.4",
    "ora": "^7.0.1",
    "sharp": "^0.32.5",
    "svgo": "^3.0.2",
    "table": "^6.8.1",
    "undici": "^5.23.0"
  },
  "devDependencies": {
    "@types/html-minifier-terser": "^7.0.0",
    "@types/node": "^20.5.7",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "watch": "pnpm build --watch",
    "try": "shx rm -fr demo && cp -R demo_rf demo && node ./dist/index.js ./demo --nocache --onlyoptim",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepublishOnly": "pnpm build",
    "cleanAllDotJampack": "find -s . | grep '.jampack$' | xargs rm -fr"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": "https://github.com/divriots/jampack/issues",
  "keywords": [],
  "types": "dist/index.d.ts"
}
